{
    "/discussion/v2/posts/:pid/vote": {
        "response": "payload",
        "isTypeConst": false,
        "obj": {
            "id": "post.pid",
            "type": "upvote",
            "version": "1.0"
        },
        "edata": {
            "type": "upvote",
            "props": "body"
        },
        "default": "downvote",
        "upvote": "upvote"
    },
    "/discussion/v2/topics": {
        "response": "payload",
        "isTypeConst": true,
        "obj": {
            "id": "topicData.tid",
            "type": "Topic",
            "version": "1.0"
        },
        "edata": {
            "type": "topic-created",
            "props": "body"
        }
    },
    "/discussion/v2/topics/:tid": {
        "response": "payload",
        "isTypeConst": false,
        "obj": {
            "id": "pid",
            "type": "parent",
            "version": "1.0"
        },
        "edata": {
            "type": "parent",
            "props": "body"
        },
        "default": "topic-replied",
        "parent": "post-replied"
    },
    "/discussion/forum/v3/create": {
        "response": "result.forums[0]",
        "isTypeConst": true,
        "obj": {
            "id": "newCid",
            "type": "Forum",
            "version": "1.0"
        },
        "edata": {
            "type": "discussion-enabled",
            "props": "body.category"
        },
        "cdata": [
            {
                "type": "sbType",
                "id": "sbIdentifier"
            },
            {
                "type": "Forum",
                "id": "newCid"  
            }
        ]
    }
}