        "notificationObj": {
            "fromUid": "payload.user.uid",
            "toUid": "payload.topic.uid",
            "category": "discussion-forum",
            "cid": "payload.cid",
            "key": "parent",
            "action": "topic",
            "types": {
                "type1": "post",
                "type2": "topic"
            },
            "actions": {
                "action1": "replied",
                "action2": ""
            },
            "params": {
                "param1": "",
                "param2": "replied",
                "param3": ""
            },
            "categoryObj": {
                "cid": "payload.cid",
                "title": "",
                "pid": ""
            },
            "topic": {
                "tid": "payload.topic.tid",
                "title": "payload.topic.title"
            },
            "post": {
                "pid": "payload.pid",
                "title": "payload.topic.title"
            }
        },
        "default": "Post Replied",
        "postData.isMain": "Topic Replied"
    },
    "/discussion/forum/v3/create": {
        "response": "result.forums[0]",
        "isTypeConst": true,
        "obj": {
            "id": "newCid",
            "type": "Category",
            "version": "1.0"
        },
        "edata": {
            "type": "Enable Discussions",
            "props": "body.category"
        },
        "cdata": {
            "type": "sbType",
