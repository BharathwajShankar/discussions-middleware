            "types": {
                "type1": "post",
                "type2": ""
            },
            "actions": {
                "action1": "downvoted",
                "action2": "upvoted"
            },
            "params": {
                "param1": "",
                "param2": "",
                "param3": ""
            },
            "categoryObj": {
                "cid": "",
                "title": "",
                "pid": ""
            },
            "topic": {
                "tid": "payload.post.tid",
                "title": "payload.post.title"
            },
            "post": {
                "pid": "payload.post.pid",
                "title": "payload.post.title"
            }
        },
        "default": "Downvoted",
        "upvote": "Upvoted"
    },
